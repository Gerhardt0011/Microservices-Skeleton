  "RabbitMqSettings": {
    "Host": "rabbitmq-clusterip-srv"
  },
  "MongoDbSettings": {
    "DatabaseName": "TeamsDB",
    "TeamsCollectionName": "Teams",
    "UsersCollectionName": "Users",
