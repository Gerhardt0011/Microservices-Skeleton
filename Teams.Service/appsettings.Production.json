{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ServiceSettings": {
    "Name": "Teams"
  },
  "Kestrel": {
    "Endpoints": {
      "webApi": {
        "Protocols": "Http1",
        "Url": "http://teams-clusterip-srv:80"
      },
      "Grpc": {
        "Protocols": "Http2",
        "Url": "http://teams-clusterip-srv:8080"
      }
    }
